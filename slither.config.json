{
  "filter_paths": [
    "node_modules",
    "lib",
    "test",
    "script",
    "monitoring"
  ],
  "exclude_detectors": [
    "incorrect-exp",
    "divide-before-multiply",
    "dangerous-strict-equalities",
    "timestamp",
    "assembly",
    "pragma",
    "solc-version",
    "low-level-calls",
    "missing-inheritance",
    "naming-convention",
    "too-many-digits",
    "unindexed-event-address",
    "uninitialized-local",
    "unused-return",
    "dead-code"
  ]
}
